<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="ring" android:innerRadiusRatio="3.0" android:thicknessRatio="7.0" android:useLevel="false">
            <solid android:color="#33ffffff" />
        </shape>
    </item>
    <item>
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0">
            <shape android:shape="ring" android:innerRadiusRatio="3.0" android:thicknessRatio="7.0" android:useLevel="true"
              xmlns:android="http://schemas.android.com/apk/res/android">
                <solid android:color="#fff5c33b" />
            </shape>
        </rotate>
    </item>
</layer-list>