<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ffc6c9cd" android:endColor="#ffd6d9de" android:angle="270.0" />
            <corners android:radius="10.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff5b92fb" android:endColor="#ff5487e7" android:angle="270.0" />
                <stroke android:width="1.0px" android:color="#ff4269b5" />
                <corners android:radius="10.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>