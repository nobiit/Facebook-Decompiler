<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <set>
            <objectAnimator android:duration="0" android:valueTo="0.95" android:valueType="floatType" android:propertyName="scaleX" />
            <objectAnimator android:duration="0" android:valueTo="0.95" android:valueType="floatType" android:propertyName="scaleY" />
        </set>
    </item>
    <item android:state_pressed="false">
        <set android:interpolator="@android:anim/overshoot_interpolator" android:shareInterpolator="true" android:ordering="together">
            <objectAnimator android:duration="100" android:valueTo="1.0" android:valueType="floatType" android:propertyName="scaleX" />
            <objectAnimator android:duration="100" android:valueTo="1.0" android:valueType="floatType" android:propertyName="scaleY" />
        </set>
    </item>
</selector>